{
  "restartable": "rs",
  "execMap": {
    "js": "node --debug=9999 --debug-brk --inspect --nolazy"
  },
  "watch": [
    "dist/main.js"
  ],
  "ignore": [
    ".git",
    "src/**/*",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "env": {
    "NODE_ENV": "development",
    "NODE_DEBUG": "http,net"
  },
  "debug": true
}